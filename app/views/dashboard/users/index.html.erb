<h3>Usuarios</h3>
<table class="table">
  <tr>
    <th><strong>Email</strong></th>
    <th><strong>Del. Domicilio</strong></th>
    <th><strong>Del. Negocio</strong></th>
    <th><strong>Perfil</strong></th>
  </tr>
<% @users.each do |user| %>
  <tr>
    <td><%= user.email %></td>
    <td><%= pretty_delegation_format(user, :home) %></td>
    <td><%= pretty_delegation_format(user, :business) %></td>
    <td><%= pretty_user_profile(user) %></td>
  </tr>
<% end %>
</table>
<%= paginate @users %>
